
public class Lab4 {

	int index = 0;
	int oddSum = 0 ;
	int evenSum  = 0;
	
	int []numbers = new int[100] ;
	
	for(int i =0; i<100; i++)
	{
		numbers[i]= i+1;
	}
	while(index <100)
	{
		index++;
		if(numbers[index]%2 ==0) evenSum+=numbers[index];
		else if(numbers[index]%2 !=0) oddSum+=oddSum[index];
	}
}
